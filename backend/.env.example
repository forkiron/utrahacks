# Server port
PORT=4000

# Gemini API (for component identification)
GEMINI_API_KEY=

# ElevenLabs (for commentary voice)
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=

# Solana (optional - skips on-chain write if not set)
SOLANA_RPC_URL=
SOLANA_PRIVATE_KEY=

# Encrypt on-chain memo (AES-256-GCM). Set to 32+ chars or 64 hex. If unset, memo is plain text.
ENCRYPTION_KEY=

# Demo mode - allows finalize without wallet signature
DEMO_MODE=true
